<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="man" class="com.zrzhao.human.impl.Man" />
    <bean name="daySleep" class="com.zrzhao.human.impl.DayDaySleep" />
    <bean name="goodEat" class="com.zrzhao.framework.aop.pointcat.GoodEat">
        <constructor-arg name="name" value="三三来迟" />
        <constructor-arg name="age" value="16" />
    </bean>

</beans>